@keyframes customIcon-spin {
	0% {
		-webkit-transform: rotate(0);
		transform: rotate(0);
	}

	100% {
		-webkit-transform: rotate(359deg);
		transform: rotate(359deg);
	}
}

.iconfont-spin {
	-webkit-animation: cuIcon-spin 2s infinite linear;
	animation: cuIcon-spin 2s infinite linear;
	display: inline-block;
}

.iconfont-pulse {
	-webkit-animation: cuIcon-spin 1s infinite steps(8);
	animation: cuIcon-spin 1s infinite steps(8);
	display: inline-block;
}

[class*="customIcon-"] {
	font-family: "customIcon";
	font-size: inherit;
	font-style: normal;
}

@font-face {
	font-family: "customIcon";
	src: url('//at.alicdn.com/t/font_2427752_3lbok2dq7z6.eot');
  src: url('//at.alicdn.com/t/font_2427752_3lbok2dq7z6.eot?#iefix') format('embedded-opentype'),
  url('//at.alicdn.com/t/font_2427752_3lbok2dq7z6.woff2') format('woff2'),
  url('//at.alicdn.com/t/font_2427752_3lbok2dq7z6.woff') format('woff'),
  url('//at.alicdn.com/t/font_2427752_3lbok2dq7z6.ttf') format('truetype'),
  url('//at.alicdn.com/t/font_2427752_3lbok2dq7z6.svg#customIcon') format('svg');
}

.customIcon-link:before {
	content: "\e6c3";
}
.customIcon-about:before {
	content: "\e688";
}
